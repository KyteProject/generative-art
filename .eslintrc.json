{
	"extends": [
		"eslint:recommended"
	],
	"rules": {
		"no-console": 0,
		"quotes": [
			"warn",
			"single"
		],
		"semi": [
			"warn",
			"always"
		]
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 8,
		"sourceType": "module",
		"ecmaFeatures": {
			"classes": true,
			"impliedStrict": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	}
}
